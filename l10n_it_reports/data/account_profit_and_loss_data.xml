        <field name="code">IT_CEA_1</field>
        <field name="sequence" eval="1" />
        <field name="level" eval="2" />
        <field name="parent_id" ref="account_financial_report_line_it_ce_A" />
        <field name="formulas">- (IT_CEA_1_V - IT_CEA_1_R)</field>
    </record>

    <record id="account_financial_report_line_it_ce_A_1_V" model="account.financial.html.report.line">
        <field name="name">value ricavi delle vendite e delle prestazioni</field>
        <field name="code">IT_CEA_1_V</field>
        <field name="level" eval="2" />
        <field name="domain" eval="['|', '|', '|', '|',
            ('account_id.code', '=like', '3101%'),
            ('account_id.code', '=like', '3102%'),
            ('account_id.code', '=like', '3103%'),
            ('account_id.code', '=like', '3104%'),
            ('account_id.code', '=like', '3105%')]" />
        <field name="groupby">account_id</field>
        <field name="formulas">sum</field>
