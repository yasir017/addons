        <field name="control_domain" eval="[('account_id.tag_ids', 'in', ref('l10n_it.account_tag_RISCHI'))]" />
        <field name="domain" eval="['|', '|','|',
            ('account_id.code', '=like', '2921%'),
            ('account_id.code', '=like', '2922%'),
            ('account_id.code', '=like', '2926%'),
